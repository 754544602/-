@charset "utf-8";
.yt-users-model {
	padding: 5px;
	height: 25px;
	width: 500px;
	border: 1px solid #D6E6F3;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	overflow: auto;
}

.yt-user-input {
	width: 75px;
	height: 25px;
	border: 0px;
}

.yt-users-list {
	display: none;
	position: relative;
	border: solid #CCCCCC 1px;
	border-top: 0px;
	height: 220px;
	overflow: auto;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}

.yt-users-list li {
	margin: 0px 5px;
	padding: 5px 0px;
	height: 20px;
	line-height: 20px;
	text-align: left;
	border-bottom: solid #EEEEEE 1px;
	cursor: pointer;
}

.yt-users-list .user-obj {
	margin: 3px;
	cursor: pointer;
}

.check-users{
	color: #FFFFFF;
	background: cornflowerblue;
}

.user-check-li {
	background: #D6E6F3;
}

.right-button-context {
	position: fixed;
	left: 0;
	top: 0;
	min-width: 100px;
	background: #fff;
	display: none;
	z-index: 9;
	border: 1px solid #D6E6F3;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}

.right-button-context .li-div {
	font-size: 12px;
	list-style: none;
	color: #333333;
	padding: 5px 10px;
	border-bottom: 1px solid #C2CCD1;
	cursor: pointer;
}

.right-button-context .li-div:hover {
	background: #D0E9F5;
}