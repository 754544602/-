<style type="text/css">
	/**
	 * 表格编辑中的thead下的th加边框
	 */
	.edit-tab{
		width: 100%;
	}
	.edit-tab .yt-thead th{
		border: 1px solid #DDDDDD;
	}
	.edit-tab .yt-tbody td{
		padding: 5px 4px;
	} 
	.edit-tab .desc-text{
		width: 390px;
		word-wrap: break-word;
	}
	/**
	 * 用div代替textarea样式
	 */
	.edit-tab .textarea-div{
		padding-left: 4px;
	    background: #FAFAFA;
	    color: #333333;
	    font-size: 14px;
	    outline: none;
	    border: 1px solid #DFE6F3;
	    -webkit-border-radius: 2px;
	    -moz-border-radius: 2px;
	    border-radius: 2px;
	    min-height: 24px;
	    outline: 0;
	    line-height: 22px;
	    word-wrap: break-word;
	    overflow-x: hidden;
	    overflow-y: auto;
	    width: 344px;
	    text-align: left;
	    cursor: default;
	    padding-top: 3px;
	    padding-bottom: 1px;
	}
	.edit-tab .textarea-div:hover {
    	border-color: #bcc8dd!important;
	}
	.edit-tab .textarea-div:focus {
    	border-color: #4387cd!important;
	}
	/**
	 * 支持表格编辑的下的input标签和select加背景色
	 */
	.edit-tab input{
		background: #FAFAFA;
		height: 28px;
		-webkit-border-radius: 2px;
	    -moz-border-radius: 2px;
	    border-radius: 2px;
	}
	.edit-tab .nice-select,.edit-tab select{
		background: #FAFAFA;
		width: 100px;
		-webkit-border-radius: 2px;
	    -moz-border-radius: 2px;
	    border-radius: 2px;
	}
	
