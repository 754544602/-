    //必带初始化内容
    <script type="text/javascript">
	    	$(function(){
	    		
	    	    //初始化开始日期
					$("#start-time").calendar({
						speed: 0, //日期列表展开显示速度, 参数"slow","normal","fast"，或毫秒数值，默认：200   
						controlId: "startDate",
						nowData:false,//默认选中当前时间,默认true
						upperLimit:$("#end-time") //开始日期最大为结束日期
					});
					//初始化结束日期
					$("#end-time").calendar({
						speed: 0, //日期列表展开显示速度, 参数"slow","normal","fast"，或毫秒数值，默认：200   
					 	controlId: "endDate",
					 	nowData:false,//默认选中当前时间,默认true
						lowerLimit:$("#start-time") //结束日期最小为开始日期
					});
						
	    	});
