<script type="text/javascript">
	$(function() {
		//动态调用主系统的换肤js
		document.getElementById("themeJs").src = $yt_themeJs_path;

		//调用上传附件方法
		$('.yt-file-up').fileUpLoad({
			titleName: '上传附件：', //title名称
			titleWid: '75px', //附件标题宽度,可为空默认100px
			fileUpId: '', //文件input框的id  默认值为 fileUp
			fileBtnId: '', //请上传按钮id 默认值为 fileBtn
			riskFontCode: '', //风险字段code值 默认值为空,是风险字段的话必须传入参数值
			callback: function(fileData) { //回调函数 返回文件数据		console.log(fileData);
			}
		});
		//图片预览
		$('.file-img-div img').showImg();
		
		
		
		//详情页面附件信息显示
		var fileList = [{
			attId: 603, 
			attName: "banner-1.jpg"
		},{
			attId: 604, 
			attName: "业务工作座谈会议人员签到表.doc"
		}]
		//判断附件集合是否有值
		if(fileList.length > 0) {
			//图片显示拼接字符串
			var ls = '';
			//图片显示路径
			var src = '';
			//遍历附件集合
			$.each(fileList, function(i, n) {
				//获取附件格式
				var imgType = n.attName.split('.');
				//判断是否是图片
				if(imgType.length > 1 && (imgType[1] == 'png' || imgType[1] == 'jpeg' || imgType[1] == 'bmp' || imgType[1] == 'jpg')) {
					//拼接图片路径调用后台接口
					//src = $yt_option.base_path + 'fileUpDownload/download?pkId=' + n.attId + '&isDownload=false';
					src = $yt_option.websit_path+'resources/images/test/banner-1.jpg';
					ls += '<div fid="' + n.attId + '" class="li-div"><span class="file-name" >' + n.attName + '</span><label class="file-dw">下载</label><label class="file-pv">预览<img src="' + src + '" ></label></div>';
				} else {
					//拼接其他文件
					ls += '<div fid="' + n.attId + '" class="li-div"><span class="file-name" >' + n.attName + '</span><label class="file-dw">下载</label></div>';
				}
			});
			$('.yt-file-info #attIdStr').html(ls);
			//附件下载操作事件
			$('.yt-file-info #attIdStr .file-dw').on('click', function() {
				//获取附件Id
				var id = $(this).parent().attr('fid');
				//请求接口执行下载操作
				//window.location.href = $yt_option.base_path + 'fileUpDownload/download?pkId=' + id + '&isDownload=true';
			});
			//调用公用的图片预览方法
			$('.yt-file-info #attIdStr .file-pv img').showImg();
		}else {
			$(".yt-file-info #attIdStr").html("暂无附件");
		}

	});
</script>