<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN"
"http://www.w3.org/TR/html4/strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">

	<head>
		<meta http-equiv="x-ua-compatible" content="ie=Edge" />
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<!--引用CSS文件START-->
		<!--滚动条样式-->
		<link rel="stylesheet" type="text/css" href="../../resources/js/scroll/jquery.mCustomScrollbar.min.css?YTVERSIONCODE=20180420" />
		<!--分页样式-->
		<link rel="stylesheet" type="text/css" href="../../resources/css/page/yt-page.min.css"/>
		<!--日期样式-->
		<link rel="stylesheet" type="text/css" href="../../resources/css/calendar/yt-calendar.css"/>
		<!--tree-->
		<link rel="stylesheet" type="text/css" href="../../resources/js/dTree/yt-dtree.min.css?YTVERSIONCODE=20180420"/>
		<!--[if lt IE 9 ]><link rel="stylesheet" type="text/css" href="../../resources/css/common/yt-common-ie.min.css"/><![endif]-->
		<!--最后引用自己页面的css样式压缩文件，且加上版本号?YTVERSIONCODE=20180420-->
		<link rel="stylesheet" type="text/css" href="../../resources/css/system/browseVol/browseQuery.min.css?YTVERSIONCODE=2019032004" />
		<!--引用CSS文件END-->
		<title>前端开发体系建设平台</title>
	</head>
	<body>
		<!--基本模型-->
		<div class="main-model">
			<!--上方表单区域START-->
				<div class="top-from">
					<div class="top-form-data">
						<label>日期：</label><input id="txtDate1" class="calendar-input" style="width: 150px;" value="" placeholder="请选择开始日期" type="text" />
						<span>一</span>
						<input id="txtDate2" class="calendar-input" style="width: 150px;" value="" placeholder="请选择结束日期" type="text" />
						<button class="yt-option-btn query-btn" onselectstart="return false" >查询</button>
						<button class="yt-assist-btn assist-btn" onselectstart="return false">重置</button>
					</div>
				</div>
			<!--上方表单区域END-->
			<!--背景颜色隔断-->
			<div class="background-border"></div>
			<!--下方汇总列表START-->
			<div class="browse-form">
				<div class="orderBy">
					<label>排序方式：</label>
					<button class="yt-option-btn query-data-btn" onselectstart="return false">按最后浏览时间倒序排序</button>
					<button class="yt-option-btn query-num-btn" onselectstart="return false">按浏览次数倒序排序</button>
				</div>
				<div class="list-data">
					<table class="yt-table payment-table cost-tab-null" style="width: 100%;">
						<thead class="yt-thead">
							<tr>
								<th>规范项/控件名称</th>
								<th>浏览次数</th>
								<th>最后浏览时间</th>
							</tr>
						</thead>
						<tbody class="yt-tbody">
						</tbody>
					</table>
					<div style="width: 100%;height: 40px;"></div>
					<!--分页-->
					<h3 class="page-ing page-num">加载更多...</h3>
				</div>
			</div>
			<!--下方汇总列表END-->
		</div>
	</body>
	<!--引用JS文件START-->
	<script type="text/javascript" src="../../resources/js/common/jquery.min.js"></script>
	<!--滚动条js-->
	<script type="text/javascript" src="../../resources/js/scroll/jquery.mCustomScrollbar.concat.min.js"></script>
	<!--日期控件-->
	<script type="text/javascript" src="../../resources/js/calendar/yt-calendar.min.js"></script>
	<!--分页js-->
	<script type="text/javascript" src="../../resources/js/page/yt-page.min.js"></script>
	<!--引用配置文件-->
	<script type="text/javascript" src="../../resources/js/common/yt-option.js"></script>
	<!--引用换肤js-->
	<script type="text/javascript" id="themeJs"></script>
	<!--引用公用文件-->
	<script type="text/javascript" src="../../resources/js/common/yt-common.min.js?YTVERSIONCODE=2018042601"></script>
	<script type="text/javascript" src="../../resources/js/common/yt-frame.min.js?YTVERSIONCODE=2018042601"></script>
	<!--引用系统公用文件-->
	<script type="text/javascript" src="../../resources/js/common/sysCommon.min.js?YTVERSIONCODE=2018042601"></script>
	<!--最后引用自己页面的js压缩文件,且加上版本号?YTVERSIONCODE=20180420-->
	<script type="text/javascript" src="../../resources/js/system/browseVol/browseQuery.min.js?YTVERSIONCODE=2019032004"></script>
	<!--引用JS文件END-->
	<script type="text/javascript">
		$(function(){
			//动态调用主系统的换主题JS
			document.getElementById("themeJs").src = $yt_themeJs_path; 
		});
	</script>
</html>