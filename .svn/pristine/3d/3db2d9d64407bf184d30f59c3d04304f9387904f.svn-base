<!--引用换肤js-->
<script type="text/javascript" id="themeJs"></script>
<script type="text/javascript" src="../../resources/js/common/yt-common.min.js"></script>
<script type="text/javascript" src="../../resources/js/common/yt-frame.min.js"></script>
<script type="text/javascript" src="../../resources/wordShow/Scripts/shCore.js"></script>
<!--引用JS文件END-->
<script type="text/javascript">
	$(function() {
		//动态调用主系统的换主题JS
		document.getElementById("themeJs").src = $yt_themeJs_path;  
		
		//测试声明一个数据对象
		var usersObj = {
			userName: "张三",
			usersAge: "25",
		};
		//点击设置值按钮操作事件
		$("button.set-val-btn").click(function() {
			//调用设置值方法,传输数据对象
			$(".users-tab-info").setDatas(usersObj);
		});
		//重置按钮操作事件
		$("button.reset-val-btn").click(function() {
			$(".users-tab-info input").val("");
			$(".users-tab-info span").text("--");
		});
	});
	$(function() {
		//点击获取值按钮操作事件
		$("button.get-val-btn").click(function() {
			//调用获取表单值的方法
			var studentObj = $(".student-info").getDatas();
			//调用设置值方法,传输数据对象
			$(".stu-get-val-model").setDatas(studentObj);
		});
	});
</script>