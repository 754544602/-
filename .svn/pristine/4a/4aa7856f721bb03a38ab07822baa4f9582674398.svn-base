<script type="text/javascript">
	$(function() {
		//动态调用主系统的换肤js
		document.getElementById("themeJs").src = $yt_themeJs_path;
		//初始化select
		$('select').niceSelect();
		//拼接自己的高级查询框表单字段
		var formHtml = '<table>' +
						'<tbody style="" class="tbody">' +
							'<tr>' +
								'<td><span style="letter-spacing: 16px;">字段</span>名：</td>' +
								'<td><input class="yt-input" id="" placeholder="请输入" style=""></td>' +
							'</tr>' +
							'<tr>' +
								'<td><span style="letter-spacing: 45px;">金</span>额：</td>' +
								'<td><input class="yt-input yt-money-input" type="text" placeholder="请输入"></td>' +
							'</tr>' +
							'<tr>' +
								'<td><span style="letter-spacing: 16px;">字段</span>名：</td>' +
								'<td>' +
									'<select class="yt-select">' +
										'<option>选项1</option>' +
										'<option>选项2</option>' +
									'</select>' +
								'</td>' +
							'</tr>' +
						'</tbody>' +
					'</table>';
		//调用公用的高级查询方法
		/**
		 *@param $(".sen-box") 存储高级查询区域对象
	 	 *@param formHtml 查询框中的表单代码
	 	 *@param $(".up-btn")  高级查询按钮对象
	     *@param query-btn 高级查询框中查询按钮类名
		 */
		$yt_controlElement.seniorQuery($(".sen-box"),formHtml,$(".up-btn"),"query-btn");
		//点击查询执行高级查询
		$('.query-btn').click(function() {
			//此处调用查询方法
		});
	});
</script>