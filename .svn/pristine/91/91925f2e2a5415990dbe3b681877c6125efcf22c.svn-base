}


/*切换键外层样式*/

.yt-spin-wrap {
	position: absolute;
	width: 20px;
	height: 24px;
	top: 4px;
	right: 0px;
}


/*向上*/

.yt-spin-up {
	background: url(../../images/icons/noUp.png) no-repeat 5px 4px;
	height: 12px;
}


/*向下*/

.yt-spin-down {
	background: url(../../images/icons/noDown.png) no-repeat 5px 4px;
	height: 12px;
}


/*向上按钮鼠标悬浮样式*/

.yt-spin-up:hover {
	background: url(../../images/icons/clickUp.png) no-repeat 5px 4px;
	height: 12px;
}


/*向下按钮鼠标悬浮样式*/

.yt-spin-down:hover {
	background: url(../../images/icons/clickDown.png) no-repeat 5px 4px;
	height: 12px;
}


/*按钮鼠标悬浮样式*/

.yt-spin:hover {
	cursor: pointer;
}


/*输入框鼠标悬浮样式*/

.yt-numberInput:hover {
	border-color: #bcc8dd!important;
}


/*输入框获取焦点样式*/

.yt-numberInput:focus {
	border-color: #4387CD!important;
}


/**
