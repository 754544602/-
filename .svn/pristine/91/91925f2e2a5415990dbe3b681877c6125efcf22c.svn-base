}
.yt-def-text{
	color: #999999;
}
/**
 * 
 *表格行样式
 *
 */

.yt-tbody tr {
	border: 1px solid #DFE6F3;
	cursor: pointer;
}


/**
 * 
 *表格列样式
 *
 */

.yt-tbody td {
	border: 1px solid #DFE6F3;
	padding: 10px 4px;
	vertical-align: middle;
	color: #333;
	font-size: 14px;
	text-align: center;
}


/**
 *表格文本过长，换行左对齐 
 *
 */

.yt-table-br {
	text-align: left;
}


/**
 * 
 * 表格行数据悬浮样式
 * 
 */

.yt-table tbody tr:hover {
	background: #fafafa;
}


/**
 * 
 *选中的行样式 
 *
 */

.yt-table-active {
	background: #d9e8f5 !important;
}


/**
 *
 *(B05)单选按钮
 *
 *
 */

label.yt-checkbox,
label.yt-radio {
	cursor: pointer;
	padding-left: 25px !important;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	height: 20px;
	display: inline-block;
}


/*label {
	display: inline-block;
	height: 22px;
}*/


/**
 * 
 * radio默认背景
 *
 */

label.yt-radio {
	background: url(../../images/icons/lsqwhite-radio.png) 0 -40px no-repeat !important;
}


/**
 * 
 * radio禁用背景
 *
 */

label.yt-radio.yt-radio-disabled {
	background: url(../../images/icons/lsqwhite-radio.png) 0 -100px no-repeat !important;
}


/**
 * 
 * 全选;多选默认样式
 * 
 */

label.yt-checkbox input,
label.yt-radio input {
	position: absolute;
	left: 0 !important;
	cursor: pointer;
	/* older safari/Chrome browsers */
	-webkit-opacity: 0;
	/* Netscape and Older than Firefox 0.9 */
	-moz-opacity: 0;
	/* Safari 1.x (pre WebKit!) 老式khtml内核的Safari浏览器*/
	-khtml-opacity: 0;
	/* IE9 + etc...modern browsers */
	opacity: 0;
	/* IE 4-9 */
	filter: alpha(opacity=0);
	/*This works in IE 8 & 9 too*/
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
	/*IE4-IE9*/
	filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=0);
}


/**
 * 
 * radio默认选中背景
 *
 */

label.yt-radio.check {
	background: url(../../images/icons/lsqwhite-radio.png) 0 0px no-repeat !important;
	position: relative;
}


/**
 * 
 * 
 * (B06)多选按钮
 *
 * 
 */


/**
 * 
 * checkbox默认背景
 *
 */

label.yt-checkbox {
	background: url(../../images/icons/white_check.png) 0 -31px no-repeat !important;
}


/**
 * 
 * checkbox默认选中背景
 *
 */

label.yt-checkbox.check {
	background: url(../../images/icons/white_check.png) 0 3px no-repeat !important;
	position: relative;
}


/**
 * 
 * checkbox禁用背景
 *
 */

label.yt-check-disabled {
	background: url(../../images/icons/white_check.png) 0 -65px no-repeat !important;
}


/**
 * 
 * 
 * checkbox半选背景
 *
 *
 */

label.yt-checkbox-half {
	background: url(../../images/icons/white_check.png) 0 -99px no-repeat !important;
}


/**
 * 
 * (B07)滚动条
 *
 */

.yt-scroll-model {
	width: 400px;
	height: 300px;
	padding: 10px;
	overflow: auto;
}


/*
 *
 *(B08)进度条
 *
 */

.yt-progress {
	padding: 6px;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	background: #f0f0f0;
	width: 260px;
	height: 12px;
	margin-bottom: 50px;
}

.yt-progress-probar {
	float: left;
	width: 260px;
	background: #d8d8d8;
	height: 12px;
	display: inline-block;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	display: none;
	position: relative;
	left: 0px;
	top: 0px;
}


/**
 * 
 * 进度条定位
 * 
 */

#yt-progress-line {
	position: absolute;
	z-index: 10px;
}


/**
 * 
 * 进度条,白色原点
 * 
 */

.yt-progress-ball-div {
	background: #fff;
	width: 14px;
	height: 14px;
	-webkit-border-radius: 50px;
	-moz-border-radius: 50px;
	border-radius: 50px;
	position: absolute;
	z-index: 20px;
	margin-top: -1px;
}


/**
 * 
 * 移动的标签进度条百分比显示
 * 
 */

.yt-progress-percent {
	position: absolute;
	left: 0px;
	top: -32px;
	display: inline-block;
	color: #fff;
	font-size: 16px;
	background: url(../../images/icons/bubble.png);
	width: 50px;
	height: 28px;
	text-align: center;
}


/**
 * 
 * 进度条背景渐变
 * 
 */

.yt-progress-line {
	float: left;
	height: 12px;
	overflow: hidden;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	background: -webkit-gradient(linear, 0 0, 0 100%, from(#b8d9fb), to(#4387cd));
	background: -webkit-linear-gradient(left, #b8d9fb, #4387cd);
	background: -moz-linear-gradient(left, #b8d9fb, #4387cd);
	background: -o-linear-gradient(left, #b8d9fb, #4387cd);
	background: -ms-linear-gradient(left, #b8d9fb, #4387cd);
	background: linear-gradient(left, #b8d9fb, #4387cd);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=1, startColorstr=#b8d9fb, endColorstr=#4387cd);
}


/*
 *
 *(B09)文本域
 *
 *
 */

.yt-textarea {
	width: 500px;
	height: 80px;
	border: 1px solid #DFE6F3;
	font-size: 14px;
	color: #333333;
	outline: none;
	background: #FFFFFF;
	padding: 4px;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	resize: none;
}


/*
 *
 * 
 *文本域悬浮样式
 *
 * 
 */

.yt-textarea:hover {
	border-color: #bcc8dd!important;
}

.yt-textarea:focus {
	border-color: #4387cd!important;
}


/*
 *
 *(B10)数字输入框
 *
 *
 */


/*外层div*/

.yt-numberInput-box {
	width: 200px;
	position: relative;
}


/*输入框样式*/

.yt-numberInput {
	color: #999;
	width: 174px;
	height: 28px;
	line-height: 28px;
	padding: 0 20px 0 4px;
	margin: 0;
	border-radius: 2px;
	outline: none;
	border: 1px solid #DFE6F3;
}

.yt-numberInput::-ms-clear {
