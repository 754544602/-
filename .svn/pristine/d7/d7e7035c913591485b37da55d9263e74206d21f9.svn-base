<script type="text/javascript">
	$(function(){
		//动态调用主系统的换肤js
		document.getElementById("themeJs").src = $yt_themeJs_path;
		
		//调用弹出框事件方法
		tabEvents();
	});
	
	function tabEvents(){
		/**
		 * 
		 * 弹出框tab事件
		 * 
		 */
		//点击新增按钮显示编辑弹出框
		$(".add-btn").click(function(){
			sysCommon.showModel($(".yt-edit-alert"));
		});
		//点击底部按钮关闭弹出框
		$(".yt-eidt-model-bottom button").click(function(){
			sysCommon.closeModel($(".yt-edit-alert"));
			
			$("body").css("overflow","initial");
		});
		//点击tab页切换
		$(".yt-model-tab li").off().on("click",function(){
			//清除其他选中样式
			$(".yt-model-tab li").removeClass("tab-check");
			//给当前选中tab添加选中样式
			$(this).addClass("tab-check");
		});
		
		/**
		 * 
		 * 审批列表tab页事件
		 * 
		 */
		/**
		 * tab标签页点击切换事件方法
		 */
		$(".yt-tab-header li").off().on("click",function(){
			//获取当前tab标签页索引
			var index = $(this).index();
			//给当前选中的tab标签添加选中样式
			$(this).addClass('active-li').siblings('li').removeClass('active-li');
			//判断tab页索引
			if(index == 1 || index == 2) {
				$('.tab-content').eq(1).css("display",'block').addClass("check").siblings('.tab-content').hide().removeClass("check");
			} else {
				$('.tab-content').eq(index).css("display",'block').addClass("check").siblings('.tab-content').hide().removeClass("check");
			}
		});
	}
</script>
